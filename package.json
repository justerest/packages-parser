{
  "name": "packages-parser",
  "version": "1.1.0",
  "description": "Gets projects dependencies and merges they to single package.json file",
  "keywords": [
    "merge",
    "dependencies",
    "package.json",
    "packages",
    "parse",
    "latest",
    "version"
  ],
  "bin": "./dist/cli.js",
  "main": "./dist/index.js",
  "typings": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/justerest/packages-parser.git"
  },
  "scripts": {
    "build": "tsc -p .",
    "start": "node dist/cli.js",
    "test": "npm run build && npm start -- -o test.json package.json"
  },
  "author": "justerest",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "command-line-args": "^5.0.2",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/node": "^10.11.4",
    "@types/node-fetch": "^2.1.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  }
}